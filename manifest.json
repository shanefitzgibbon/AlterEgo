{
    "manifest_version": 3,
    "name": "AlterEgo",
    "version": "1.0.0",
    "permissions": [
        "cookies",
        "storage",
        "tabs",
        "browsingData",
        "sidePanel"
    ],
    "background": {
        "service_worker": "src/background/index.js",
        "type": "module"
    },
    "side_panel": {
        "default_path": "src/sidepanel/index.html"
    },
    "action": {
        "default_title": "Open Side Panel"
    }
}